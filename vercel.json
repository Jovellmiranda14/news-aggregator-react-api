{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/api",
      "dest": "api/api.js"
    },
    {
      "src": "/api/article",
      "dest": "api/article.js"
    },
    {
      "src": "/api/weather",
      "dest": "api/weather.js"
    },
    {
      "src": "/api/weather/(.*)",
      "dest": "api/weather.js"
    }
  ]
}
